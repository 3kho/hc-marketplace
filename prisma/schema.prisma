generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  slackId     String @id
  airtable_id String
  token       String
}

// cache because we cannot depend on airtable
model CacheObject {
  id   String   @id @default(uuid())
  key  String
  ttl  DateTime @default(dbgenerated("NOW() + interval '1 hour'"))
  data Json // what u thought i was a good coder??
}
